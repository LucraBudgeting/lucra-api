generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DIRECT_DATABASE_URL")
}

// If a full DB reset needs to happen, you will need to delete the stored data types
enum BillingStatus {
    PENDING
    COMPLETED
    FAILED
}

enum AccountStatus {
    ACTIVE
    INACTIVE
    ONBOARDING
    DELINQUENT
}

model User {
    id    Int     @id @default(autoincrement())
    email String  @unique
    name  String?
}
